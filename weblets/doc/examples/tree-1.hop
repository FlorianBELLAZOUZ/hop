;; A static tree
(let ((img "data:image/png;base64,"))
   (<HTML>
      (<HEAD> :include "hop-tree")
      (<BODY>
	 (<TREE> :id "computer-science"
		 :open #t
		 (<TRHEAD> "Computer Science")
		 (<TRBODY>
		    (<TRLEAF> (<A> :href "http://www.acm.org" "ACM") :value "acm")
		    (<TRLEAF> (<A> :href "http://www.inria.fr" "INRIA") :value "inria")
		    (<TREE>
		       (<TRHEAD> "Programming languages")
		       (<TRBODY>
			  (<TREE>
			     (<TRHEAD> "Functional")
			     (<TRBODY>
				(<TRLEAF> "Haskell")
				(<TRLEAF> "Hop")
				(<TRLEAF> "Lisp")
				(<TRLEAF> "ML")))
			  (<TREE> :foldero img :folderc img
				  (<TRHEAD> "Imperative")
				  (<TRBODY>
				     (<TRLEAF> "Ada")
				     (<TRLEAF> "C")
				     (<TREE>
					(<TRHEAD> "Object oriented")
					(<TRBODY>
					   (<TRLEAF> "C++")
					   (<TRLEAF> "Java")))
				     (<TRLEAF> "Pascal")))
			  (<TRLEAF> "APL"))))))))
