((name "info")
 (author "M. Serrano")
 (version "@VERSION@")
 (date "@DATE@")
 (title "General informations on the running Hop.")
 (about-service "about")
 (rc (when (> (bigloo-debug) 0) (hop-load-once (make-file-path (hop-weblets-directory) "info" "info.hop"))))
 (autoload (let ((pred (autoload-prefix "/hop/info")))
	      (lambda (req)
		 (and (pred req) (= (bigloo-debug) 0))))))
